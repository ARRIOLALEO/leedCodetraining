var arrayStringsAreEqual = function(word1, word2) {
    word1 = [...word1.join("")]
    word2 =[...word2.join("")]
    if(word1.length === word2.length){
        for(let i =0 ; i < word1.length ; i++){
            if(word1[i] !== word2[i]){
                return false
                break
            }
        }
    }else{
        return false
    }
   return true
};
